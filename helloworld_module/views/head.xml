<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="head_meta" inherit_id="website.layout">
        <xpath expr="//head/title" position="replace">
            <title> ToDo LLC </title>
        </xpath>

        <xpath expr="//head" position="inside">

            <link rel="shortcut icon" type="image/x-icon" href="/helloworld_module/static/src/img/logo.png" />
            <script type="text/javascript" src="/helloworld_module/static/src/js/helpers.js"></script>

        </xpath>

        <xpath expr="//main" position="inside">

            <t t-call="helloworld_module.s_dialog_form" />
            <t t-call="helloworld_module.s_contact_modal_form" />

        </xpath>

