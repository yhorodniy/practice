<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mywebsite_homepage" model="website.page">
        <field name="name">My Website Homepage</field>
        <field name="url">/</field>
        <field name="menu_id" ref="mywebsite_menu"/>
        <field name="is_home" eval="True"/>
        <field name="page_type">home</field>
        <field name="content" type="html">
            <h1>Welcome to My Website</h1>
            <p>This is the homepage of my website.</p>
        </field>
    </record>

    <record id="mywebsite_aboutpage" model="website.page">
        <field name="name">My Website About Page</field>
        <field name="url">/about</field>
        <field name="menu_id" ref="mywebsite_menu"/>
        <field name="page_type">about</field>
        <field name="content" type="html">
            <h1>About My Website</h1>
            <p>This is the about page of my website.</p>
        </field>
    </record>
</odoo>