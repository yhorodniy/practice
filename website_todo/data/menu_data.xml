<odoo>
    <data>

        <record id="website.homepage" model="ir.ui.view">
            <field name="active" eval="True"/>
        </record>

        <record id="website.contactus" model="ir.ui.view">
            <field name="active" eval="False"/>
        </record>
        
        <record id="menu_modules" model="website.menu">
            <field name="name">Модулі</field>
            <field name="url">/modules</field>
        </record>

        <record id="menu_integrations" model="website.menu">
            <field name="name">Інтеграції</field>
            <field name="url">/#integrations</field>
        </record>

        <record id="menu_cases" model="website.menu">
            <field name="name">Кейси</field>
            <field name="url">/#cases</field>
        </record>

        <record id="menu_contacts" model="website.menu">
            <field name="name">Контакти</field>
            <field name="url">/contacts</field>
        </record>

    </data>
</odoo>